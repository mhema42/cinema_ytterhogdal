<nav class="movies-nav">
    <p>Information om filmen</p>
</nav>

<div class="movie-space">
  <div class="movie-info">
    <article class="movie">
    <h1>{{ movie.attributes.title }}</h1>
    <div class="intro">
      {{{ markdown movie.attributes.intro }}}
    </div>
    {{#if movie.attributes.image }}
    <img src="{{ movie.attributes.image.url }}">
    {{/if }}
  </div>
</div>

<script src="https://kit.fontawesome.com/955d716fd8.js" crossorigin="anonymous"></script>
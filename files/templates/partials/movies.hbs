<div class="movie-space">
    <div class="all-movies-list movies-item">
        <ul>
            {{#each movies }}
            <li>
                <a href="/movies/{{ this.id }}">
            {{ this.title }}
            </a>
            </li>
            {{/each}}
        </ul>
    </div>

    <div class="popular-movies-list movies-item movies-hide">
        {{>movies/popular}}
    </div>

    <div class="current-movies-list movies-item movies-hide">
        {{>movies/current}}
    </div>

    <div class="upcoming-movies-list movies-item movies-hide">
        {{>movies/upcuming}}
    </div>
</div>

